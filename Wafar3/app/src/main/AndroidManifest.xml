<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.wafar">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Wafar">
        <activity android:name=".ProductDescription"></activity>
        <activity android:name=".Seller" />
        <activity android:name=".search_product" />
        <activity android:name=".addproduct" />
        <activity android:name=".ProductConifmation" />
        <activity android:name=".SignUp" />
        <activity android:name=".LogIn" />
        <activity android:name=".PlaceOrder" />
        <activity android:name=".Cart" />
        <activity android:name=".Cameras" />
        <activity android:name=".Departments" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>